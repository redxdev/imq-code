{
    "name": "imq-code",
    "displayName": "imquery",
    "description": "imquery language support",
    "version": "0.0.1",
    "publisher": "redxdev",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Languages"
